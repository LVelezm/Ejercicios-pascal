program caso3;
var 
	num: integer;
begin
	writeln('Ingrese un numero del 1 al 12 :');
	readln(num);
	if (num<=0) or (num>12) then
		writeln('Numero incorrecto')
	else
		case (num) of
		1: writeln('Enero');
		2: writeln('Febrero');
		3: writeln('Marzo');
		4: writeln('Abril');
		5: writeln('Mayo');
		6: writeln('Junio');
		7: writeln('Julio');
		8: writeln('Agosto');
		9: writeln('Septiembre');
		10: writeln('Octubre');
		11: writeln('Noviembre');
		12: writeln('Diciembre');
	end;
end.