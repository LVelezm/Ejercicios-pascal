program caso5;
var 
	num: integer;
begin
	writeln('Ingrese un numero del 1 al 7 :');
	readln(num);
	if (num<=0) or (num>7) then
		writeln('Numero incorrecto')
	else
		case (num) of
		1: writeln('Lunes');
		2: writeln('Martes');
		3: writeln('Miercoles');
		4: writeln('Jueves');
		5: writeln('Viernes');
		6: writeln('Sabado');
		7: writeln('Domingo');
	end;
end.